        <value>(function execute(inputs, outputs) {
	try{
      	// EC2 API version
  		var version='2016-11-15';
      
      	var action='DescribeInstanceStatus';
      
      	var region='us-east-1'; // default - should be overwritten below
      	
      	outputs.version = version;
      	outputs.action = action;
      	
      	// Path for the endpoint to use for signing step
      	outputs.path = '?Action=' + action + '&amp;InstanceId.1=' + inputs.instanceid + '&amp;Version=' + version;
        
      	// Get the region of the instance ID specified in the action
      	var vmGr = new GlideRecord('cmdb_ci_vm_instance');
		vmGr.addQuery('object_id', inputs.instanceid);
		vmGr.query();

		if(vmGr.next()){
 			var typeGr = new GlideRecord('cmdb_rel_type');
    		typeGr.addQuery('name', 'Hosted on::Hosts');
    		typeGr.query();
    		if(typeGr.next()){
      			var tsys = typeGr.sys_id;
    		}
    
    		var relGr = new GlideRecord('cmdb_rel_ci');
    		var strQuery = 'parent.sys_id='+vmGr.sys_id;
    		strQuery = strQuery + '^type.sys_id='+tsys;
    		relGr.query();
    		if(relGr.next()){
      			region = relGr.child.name;
    		}
		}
      	outputs.region = region;
    }
  	catch (e) {
    	gs.error("Error building endpoint for Get Instance State action: " + e);
    }
})(inputs, outputs);
</value>
